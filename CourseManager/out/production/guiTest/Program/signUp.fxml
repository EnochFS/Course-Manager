<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="596.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Program.SignUp">
   <children>
      <Button fx:id="backPage" layoutX="165.0" layoutY="507.0" mnemonicParsing="false" onAction="#closeSignUpApplication" text="Back" />
      <Button fx:id="finalizeProfile" layoutX="378.0" layoutY="507.0" mnemonicParsing="false" onAction="#finalizeProfile" text="Confirm" />
      <TextField fx:id="firstName" layoutX="89.0" layoutY="89.0" prefHeight="25.0" prefWidth="233.0" />
      <TextField fx:id="lastName" layoutX="89.0" layoutY="158.0" prefHeight="25.0" prefWidth="233.0" />
      <TextField fx:id="newUserName" layoutX="89.0" layoutY="225.0" prefHeight="25.0" prefWidth="233.0" />
      <Label layoutX="425.0" layoutY="93.0" text="First Name" />
      <Label layoutX="425.0" layoutY="162.0" text="Last Name" />
      <Label layoutX="425.0" layoutY="228.0" text="Username" />
      <Label layoutX="427.0" layoutY="300.0" text="Password" />
      <Label layoutX="425.0" layoutY="374.0" text="Confirm Password" />
      <RadioButton fx:id="male" layoutX="162.0" layoutY="450.0" mnemonicParsing="false" selected="true" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="GenderRadio" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="female" layoutX="335.0" layoutY="450.0" mnemonicParsing="false" text="Female" toggleGroup="$GenderRadio" />
      <PasswordField fx:id="firstPass" layoutX="91.0" layoutY="296.0" prefHeight="25.0" prefWidth="233.0" />
      <PasswordField fx:id="confPass" layoutX="91.0" layoutY="370.0" prefHeight="25.0" prefWidth="233.0" />
   </children>
</AnchorPane>
